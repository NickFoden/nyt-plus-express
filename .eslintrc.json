{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "camelcase":"off",
        "max-len":"off",
        "new-cap":"off",
        "object-curly-spacing":"off" ,
        "quotes":["error", "double"],
        "require-jsdoc": ["off"]
    }
}